{
  "symbol-whitelist" : [
    "null", "true", "false",
    "static", "self", "parent",
    "array", "string", "int", "float", "bool", "iterable", "callable", "void", "object", "mixed", "never", "app", "env","go",
    "find_binary", "is_root", "get_namespace_from_file", "is_phar", "is_cosmic", "cypher", "decipher", "find_env",
    "value", "git_config",
    "PhpOption\\Option",
    "Psr\\Container\\ContainerInterface"

  ],
  "php-core-extensions" : [
    "Core",
    "date",
    "pcre",
    "Phar",
    "Reflection",
    "SPL",
    "standard"
  ],
  "scan-files" : ["helper/*.php"]
}

