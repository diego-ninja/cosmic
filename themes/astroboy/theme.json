{
  "name": "astroboy",
  "version": "1.0.0",
  "description": "Default Cosmic Terminal Theme",
  "colors": {
    "text": "#fdf6e3",
    "comment": "#EBCB8B",
    "info": "#2aa198",
    "notice": "#268bd2",
    "warning": "#cb4b16",
    "error": "#dc322f",
    "success": "#859900",
    "red": "#dc322f",
    "green": "#859900",
    "yellow": "#b58900",
    "blue": "#268bd2",
    "purple": "#859900",
    "cyan": "#88C0D0",
    "critical": {
      "fg": "#dc322f",
      "options": ["blink"]
    },
    "warn": {
      "fg": "#cb4b16",
      "options": ["blink"]
    },
    "running": {
      "fg": "#fdf6e3",
      "options": ["blink"]
    }
  },
  "icons": {
    "application": "\uD83D\uDE80",
    "bullet": "⬢",
    "success": "⬡",
    "failure": "⬡"
  },
  "styles": {
    "container" : "w-120 ml-2",
    "footer" : "w-120 pt-2 ml-2",
    "emphasis" : "font-bold text-yellow italic",
    "strong" : "font-bold text-blue",
    "strike" : "line-through",
    "hr" : "text-blue",
    "h1" : "invisible",
    "heading" : "text-blue pt-1",
    "language-sh" : "w-120",
    "language-php" : "w-120",
    "language-js" : "w-120",
    "language-sql" : "w-120",
    "language" : "w-120",
    "link" : "text-blue underline",
    "app-icon" : "mr-1",
    "question" : "text-blue",
    "default-option" : "font-bold text-blue"
  },
  "config": {
    "icons_enabled": true
  }
}

