{
    "name": "alien",
    "version": "1.0.0",
    "description": "Extraterrestrial Terminal Theme for Cosmic",
    "extends": "default",
    "colors": {
        "text": "#FEFFFC",
        "comment": "#96CA4B",
        "info": "#749441",
        "notice": "#246B73",
        "warning": "#D08770",
        "error": "#854731",
        "success": "#96CA4B",
        "white": "#FFFFFF",
        "black": "#000000",
        "grey": "#3B4251",
        "red": "#854731",
        "green": "#96CA4B",
        "yellow": "#E5DEBB",
        "blue": "#81A1C1",
        "purple": "#3B4251",
        "cyan": "#88C0D0",
        "critical": {
            "fg": "#854731",
            "options": [
                "blink"
            ]
        },
        "warn": {
            "fg": "#CEA061",
            "options": [
                "blink"
            ]
        }
    },
    "icons": {
        "application": "\uD83D\uDC7D",
        "bullet": "⬢",
        "success": "⬡",
        "failure": "⬡"
    },
    "styles": {
        "container": "w-120 ml-2",
        "footer": "w-120 pt-2 ml-2",
        "emphasis": "font-bold text-yellow italic",
        "strong": "font-bold text-blue",
        "strike": "line-through",
        "hr": "text-blue",
        "h1": "invisible",
        "heading": "text-blue pt-1",
        "language-sh": "w-120",
        "language-php": "w-120",
        "language-js": "w-120",
        "language-sql": "w-120",
        "language": "w-120",
        "link": "text-blue underline",
        "app-icon": "mr-1",
        "question": "text-blue",
        "default-option": "font-bold text-blue"
    },
    "config": {
        "icons_enabled": true,
        "table": {
            "charset": {
                "top": "─",
                "top-mid": "┬",
                "top-left": "┌",
                "top-right": "┐",
                "bottom": "─",
                "bottom-mid": "┴",
                "bottom-left": "└",
                "bottom-right": "┘",
                "left": "│",
                "left-mid": "├",
                "mid": "─",
                "mid-mid": "┼",
                "right": "│",
                "right-mid": "┤",
                "middle": "│"
            },
            "table_color": "white",
            "header_color": "green",
            "field_color": "blue"
        }
    }
}
